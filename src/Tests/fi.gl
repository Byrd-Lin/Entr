extern int printf(char *str, ...);
extern void *malloc(int:64 size);

int fi(int n, int m)
{
	if (n == 1) {
		return m;
	}
	if (m == 1) {
		return n;
	}

	return fi(n - 1, m) + fi(n, m -1);
}

